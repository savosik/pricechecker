{
    "name": "moonshine/moonshine",
    "description": "Laravel administration panel",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "admin",
        "dashboard",
        "moonshine"
    ],
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "info@cutcode.ru",
            "homepage": "https://cutcode.dev"
        }
    ],
    "homepage": "https://getmoonshine.app",
    "support": {
        "issues": "https://github.com/moonshine-software/moonshine/issues",
        "source": "https://github.com/moonshine-software/moonshine"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/contracts": "^10|^11|^12",
        "illuminate/filesystem": "^10|^11|^12",
        "illuminate/support": "^10|^11|^12",
        "illuminate/view": "^10|^11|^12",
        "lee-to/fast-attributes": "^0.3",
        "lee-to/laravel-package-command": "^0.9",
        "nyholm/psr7": "^1.8",
        "psr/container": "^2.0",
        "psr/http-message": "^2.0",
        "psr/simple-cache": "^3.0",
        "symfony/http-foundation": "^6.2|^7.1|^7.2|^7.3",
        "symfony/psr-http-message-bridge": "^6.2|^7.1|^7.2|^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "larastan/larastan": "^3.4",
        "moonshine/import-export": "^2.0",
        "orchestra/testbench": "^9.9",
        "pestphp/pest": "^4",
        "pestphp/pest-plugin-laravel": "^4",
        "rector/rector": "~2.2.0",
        "symplify/monorepo-builder": "^11.2"
    },
    "replace": {
        "moonshine/asset-manager": "self.version",
        "moonshine/color-manager": "self.version",
        "moonshine/contracts": "self.version",
        "moonshine/core": "self.version",
        "moonshine/crud": "self.version",
        "moonshine/laravel": "self.version",
        "moonshine/menu-manager": "self.version",
        "moonshine/support": "self.version",
        "moonshine/ui": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MoonShine\\": "src/",
            "MoonShine\\AssetManager\\": "src/AssetManager/src",
            "MoonShine\\ColorManager\\": "src/ColorManager/src",
            "MoonShine\\Contracts\\": "src/Contracts/src",
            "MoonShine\\Core\\": "src/Core/src",
            "MoonShine\\Crud\\": "src/Crud/src",
            "MoonShine\\Laravel\\": "src/Laravel/src",
            "MoonShine\\Laravel\\Database\\Factories\\": "src/Laravel/database/factories/",
            "MoonShine\\MenuManager\\": "src/MenuManager/src",
            "MoonShine\\Support\\": "src/Support/src",
            "MoonShine\\UI\\": "src/UI/src"
        },
        "files": [
            "src/Laravel/src/helpers.php",
            "src/Support/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "MoonShine\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonShine\\Laravel\\Providers\\MoonShineServiceProvider"
            ]
        }
    },
    "scripts": {
        "rector": "vendor/bin/rector process --clear-cache",
        "rector:dry": "vendor/bin/rector process --clear-cache --dry-run",
        "test": "vendor/bin/pest",
        "test:bail": "vendor/bin/pest --bail",
        "test:fresh": [
            "rm -rf vendor/orchestra",
            "composer install",
            "vendor/bin/pest --bail"
        ],
        "test:all": [
            "@rector",
            "@cs:fix",
            "@test:fresh",
            "@test:types",
            "@test:types:packages"
        ],
        "test:types": "vendor/bin/phpstan analyse src --memory-limit=-1",
        "test:types:cm": "vendor/bin/phpstan analyse src/ColorManager/src --memory-limit=-1 --level=max",
        "test:types:am": "vendor/bin/phpstan analyse src/AssetManager/src --memory-limit=-1 --level=max",
        "test:types:c": "vendor/bin/phpstan analyse src/Contracts/src --memory-limit=-1 --level=max",
        "test:types:core": "vendor/bin/phpstan analyse src/Core/src --memory-limit=-1 --level=max",
        "test:types:s": "vendor/bin/phpstan analyse src/Support/src --memory-limit=-1 --level=max",
        "test:types:mm": "vendor/bin/phpstan analyse src/MenuManager/src --memory-limit=-1 --level=max",
        "test:types:crud": "vendor/bin/phpstan analyse src/Crud/src --memory-limit=-1 --level=6",
        "test:types:ui": "vendor/bin/phpstan analyse src/UI/src --memory-limit=-1 --level=6",
        "test:types:l": "vendor/bin/phpstan analyse src/Laravel/src --memory-limit=-1 --level=5",
        "test:types:packages": [
            "@test:types:am",
            "@test:types:cm",
            "@test:types:c",
            "@test:types:mm",
            "@test:types:s",
            "@test:types:core",
            "@test:types:crud",
            "@test:types:ui",
            "@test:types:l"
        ],
        "cs:fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "monorepo:merge": "vendor/bin/monorepo-builder merge"
    }
}
